/**index.wxss**/
.btn1 {
    position: relative;
    display: block;
    width: 700rpx;
    height: 400rpx;
    margin-bottom: 20rpx;
    margin-left: 0;
    margin-right: 0;
    box-sizing: border-box;
    font-weight: 800;
    font-size: 50rpx;
    text-align: center;
    text-decoration: none;
    line-height: 400rpx;
    border-radius: 20rpx;
    padding: 0 0 0 0;
    background-image: url('http://120.25.123.46/media/index/index1.png');
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
